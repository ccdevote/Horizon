<script type="text/javascript">
var S = {
$: function(id){return document.getElementById(id)},
tag: function(tag, container){
container = container || document;
return container.getElementsByTagName(tag);
},
extend: function (destination, source, override) {
for (var property in source) {
if (!override || !(property in destination))
destination[property] = source[property];
}
return destination;
}
}
function Slide(config){
var defaultConf = {
width: 876,
auto: false,
speed: 10,
interval: 4000
},
c = S.extend(config, defaultConf, true);
this.config = c;
this.init(c);
}
Slide.prototype = {
init: function(c) {
var self = this;
this.y = 0;
this.num = S.tag('li', c.target).length;
if (c.auto) self.play();
c.target.onmouseover = function() {
self.stop();
};
c.target.onmouseout = function() {
self.play();
};
},
posTo: function(y) {
var c = this.config;
this.y = y;
c.target.style.marginLeft = this.y + 'px';
},
scrollTo: function(y, fn) {
if (this.timer) clearInterval(this.timer);
var dis,
end = y,
self = this;
this.timer = setInterval(function() {
dis = (end - self.y) / self.config.speed;
var to = self.y + dis;
if (Math.round(to) == end) {
to = end;
clearInterval(self.timer);
if (fn) fn.call(self);
var control = self.config.control;
var nowActive = -to / self.config.width;
//console.log("active: " + nowActive);
self.activeBtn(control, nowActive);
}
self.posTo(to)
},
13);
},
nextGroup: function(){
var c = this.config;
this.n++;
if(this.n >= this.num) this.n = 0;
//console.log(this.n);
//console.log(parseInt(-this.n * c.perGroup * c.singleWidth));
this.activeBtn(c.control, this.n);
this.scrollTo(parseInt(-this.n * c.perGroup * c.singleWidth));
},
prevGroup: function(){
var c = this.config;
this.n -= 1;
if(this.n < 0) this.n = 2;
//console.log(this.n);
//console.log(parseInt(-this.n * c.perGroup * c.singleWidth));
this.activeBtn(c.control, this.n);
this.scrollTo(-this.n * c.perGroup  * c.singleWidth);
},
activeBtn: function(c, n) {
for (var i = 0; i < c.length; i++) {
c[i].className = ''
}
c[n].className = 'active';
},
play: function() {
var self = this;
self.autoTimer = setInterval(function() {
if (self.n == undefined) self.n = 1;
if (self.n >= self.num) self.n = 0;
self.scrollTo( - (self.n++) * self.config.width);
},
self.config.interval)
},
stop: function() {
clearInterval(this.autoTimer);
}
}
var miniSlide = new Slide({
target: S.tag('ul', S.$('slide'))[0],
control: S.tag('a',S.$('slide_control')),
width: 876,
singleWidth:219,
perGroup:4,
interval: 5000,
auto: false
});
miniSlide.num = 3;
miniSlide.n = 0;
</script>
