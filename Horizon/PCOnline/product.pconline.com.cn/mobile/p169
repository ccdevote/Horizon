IPInfo.callBack = function(siteInfo){
	var siteId = siteInfo[3];
	// 当识别出来的siteId为北上广深 的时间执行if 为 true的块,否则执行else 块
	if(siteId == 1 || siteId == 2 || siteId ==3 || siteId ==4){
		$.get('http://product.pconline.com.cn/pdlib/service2011/price_dealer.jsp?areaId=100&brandId=22586&qq&time=900&priority&groupType=3&areaIds='+",1,2,3,4,",function(json){
			setPriceAdJson(json,4,siteInfo);
		},"json");
	}else {
		$.get('http://product.pconline.com.cn/pdlib/service2011/price_dealer.jsp?areaId=100&brandId=22586&qq&time=900&priority&groupType=3&areaIds='+",1,2,3,4,"+siteId+",",function(json){
			var tabName = siteInfo[2];
			$("#tabfzn").removeAttr("style");
			$("#tabfzn").text(tabName);
			setPriceAdJson(json,4,siteInfo);
		},"json");
	}
	
			var str = getOtherCityStr(IPInfo.site);
			
			$('#imOtherCity').html(str);
			$('#otherHotCity').html(str);
};
IPInfo.exec();
