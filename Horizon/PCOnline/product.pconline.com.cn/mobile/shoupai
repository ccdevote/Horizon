			});
		}

		 if(jumpAreaId==0){
				var info =IPInfo.groups[IPInfo.site];
				showCurrent(IPInfo.site);
				showRelated(info.related,info.id);
				showHot(IPInfo.hot,info.id);
		}else{
			var info = IPInfo.groups[jumpArea];
			showCurrent(jumpArea);
			showRelated(info.related,info.id);
