<script>
shareBox.init({
	show:["tools","top"],
	clickTools : function() {
		if ($("#compareBarOut").hasClass("hide") || $("#JtoolBar").hasClass("hide")) {
			$("#compareBarOut").removeClass("hide");
			$("#JtoolBar").removeClass("hide");
			$("#JtoolBar .pro").eq(0).removeClass("hide");
			$("#JtoolBar .pro").eq(1).addClass("hide");
			$("#JtoolBar .tab i").eq(0).addClass("on");
			$("#JtoolBar .tab i").eq(1).removeClass("on");
			//syncWidth();
		} else {
			$("#compareBarOut").addClass("hide");
		}
	}
});
</script>
