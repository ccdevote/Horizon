<script>
var getid = function(id){ return document.getElementById(id)};
(function(){
	
	/* 产品列表 */
	var tags = getid("Jproduct-list").getElementsByTagName("a"),ocur=getid("showBigPic"),
	j=0,i=0,len = tags.length,atag=[],t,l,t2;
	for(;i<len;i++){if(tags[i].className.indexOf("aPic")!=-1){atag.push(tags[i])}	}
	hoverDelay(ocur,function(){clearTimeout(ocur.o.t)},function(){
		t2 = setTimeout(function(){
		ocur.className="hide"; ocur.o.className="aPic";
		},10)
	},10);	
	for(;l=atag.length,j<l;j++){
		hoverDelay(atag[j],function(){
			if(ocur.o ===this){clearTimeout(t2)};
			var bigpicUrl = getTag(this,"img").getAttribute("data-src");
			if(bigpicUrl=="null"){return}else{getTag(ocur,"img").src=bigpicUrl;getTag(ocur,"a").href=this.href;}
			l=getElementLeft(this.parentNode);	t=getElementTop(this.parentNode);
			this.className="aPic ahover";ocur.className="show";
			ocur.style.left=l+130+"px";	ocur.style.top=t-65+"px";
			ocur.o = this;
		},function(){
			this.className="aPic";	ocur.className="hide";
			getTag(ocur,"img").src = "http://www1.pconline.com.cn/2011/product/img/init/loading.jpg";
			ocur.o = this;
		},300)
	}
	function getTag(o,tag){return o.getElementsByTagName(tag)[0]}
	function getElementLeft(element){
		var ol = element.offsetLeft,current = element.offsetParent;
		while (current !== null){ol += current.offsetLeft;	current = current.offsetParent}	return ol;
	}
	function getElementTop(element){
		var ot = element.offsetTop,current = element.offsetParent;
		while (current !== null){	ot += current. offsetTop;current = current.offsetParent;}return ot;
	}
	
	
	/*条件筛选更多*/
	var odds = getid("J-getWap").getElementsByTagName("dd");
	for(var i=0,ddl=odds.length;i<ddl;i++){
		if(odds[i].className.indexOf("moreitem")!=-1 || odds[i].className.indexOf("lessitem")!=-1){
			odds[i].onclick=function(){
				if(this.className.indexOf("moreitem")!=-1){
					this.innerHTML="收起";
					this.parentNode.getElementsByTagName("dd")[0].className="clearfix";
					this.className="lessitem";
				}else{
					this.innerHTML="更多";
					this.parentNode.getElementsByTagName("dd")[0].className="clearfix less";
					this.className="moreitem"
				}
			}	
		}
	}
	/* 条件筛选更多 end */
	var ocp = getid("Jchange-pop");
	//getid("Jchange").onclick=function(){ocp.className="change-pop show"}
	hoverDelay(ocp,function(){this.className="change-pop show"},function(){this.className="change-pop hide"})
	mousehover("Jproduct-list","li","on");
	
	function SetCookie(name,value,Days){
		var exp  = new Date();	exp.setTime(exp.getTime() + Days*24*60*60*1000);
		document.cookie = name + "="+ escape (value) + ";path=/;expires=" + exp.toGMTString();
	}
	function getCookie(name){
		var arr = document.cookie.match(new RegExp("(^| )"+name+"=([^;]*)(;|$)"));
		if(arr != null) return unescape(arr[2]); return null;
	}
	
	//品牌大于defBrandCnt个时才弹出多选的提示
	if(!getCookie("pcPriceBrandPop")&&!false){showPop();}
	
	$("#JcosBrand a").click(function(){hidePop();SetCookie("pcPriceBrandPop","true",15);});
	$("#JcosBrand i").click(function(){hidePop();SetCookie("pcPriceBrandPop","true",15);});	
	function hidePop(){$("#JcosBrand").fadeOut("fast")}
	function showPop(){
		var aoff = $("#J-multiBtn").offset();
		$("#JcosBrand").fadeIn("fast").css({left:aoff.left-172,top:aoff.top+16});
	}
	window.setTimeout(function(){
		hidePop();
	}, 10000);
})();


function compareProduct(obj, pubUrl, shorName, picUrl, price, smallTypeId, id){
	if(obj.className == 'checked') {
		obj.className = '';
		
	} else {
		obj.className = 'checked';
		compareBar.comparePro(obj,pubUrl, shorName, picUrl, price, smallTypeId, id);
	}
	
}
