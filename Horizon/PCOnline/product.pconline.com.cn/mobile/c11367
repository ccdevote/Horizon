function setMutiLink() {
	try {
		if(allBrandJson) {
			$('#J-allBrandList a').each(function(){
				//mlink_24933
				var id = $(this).attr('id');
				var link = eval('allBrandJson.'+id);
				/*if(link && link.indexOf('multiple_') == -1) {
					link += 'multiple_' + id.substring(6) + '/'
				}*/
				//alert(link);
				$(this).attr('href', link);
			});
		}		
	} catch(e) {
	}
}


		var ppcachesrc = "http://ppcache.pconline.com.cn/priceCache/cache?method=readjs&productIds=|489039|478263|489352|497242|503727|485270|478134|498967|494478|496068|498783|496894|506743|496069|497222|500418|505164|502071|499227|499920|496215|504492|490701|497291|490052&areaId="+_areaId;


$.getScript(ppcachesrc, function() {
	var result = getPrices();
	//产品数组
	var pds = result.products;//array [Product(441754,3150.0,5.04,1,145,2639,3599,0), ...]
    var price;
    var changerate;
    var changerate_class;
	for(var i = 0, c = pds.length; i < c; ++i) {
		//产品地区报价
        if(pds[i].price == 0.0) {
            price = "";
    		changerate = "D";
    		changerate_class = "black";
        } else {
        	price = pds[i].price;
            if (pds[i].changeRate == 0.0) {
    			changerate = "D";
    			changerate_class = "black";
            } else if (pds[i].changeRate < 0.0) {
                changerate = "↓" + (pds[i].changeRate * -1) + "%";
    			changerate_class = "green";
            } else {
                changerate = "↑" + pds[i].changeRate + "%"
    			changerate_class = "red";
            }
	    }
	    if(price) {
	    	var price_pds = $("#price_" + pds[i].id).get(0);
        	if(price_pds) price_pds.innerHTML = '￥' + '<em>' + price + '</em>';
        	//alert(price_pds.innerHTML);
    	} else {
        	var price_pds = $("#price_" + pds[i].id).get(0);
        	if(price_pds) {
        		var txt = price_pds.innerHTML;
        		if(txt != '<em>新产品</em>') price_pds.innerHTML = "<em>缺货</em>";
        	}
        }
        //修改报价趋势
        var changerate_pds = $("#changerate_" + pds[i].id).get(0);
        if(changerate_pds) {
        	changerate_pds.innerHTML = changerate;
        	changerate_pds.className = changerate_class;
        }
        
		if(_areaId != 100){//不是全国
    		var price_pds = $("#price_" + pds[i].id).get(0);
        	if(price_pds) {
        		price_pds.href = "/pdlib/"+pds[i].id+"_price_"+_areaId+".html"; //修改报价链接
            }
        }
	}
	
	//报价地区显示
	$('#city').get(0).innerHTML = _areaName+'指导价';
	
});
//验证自定义价格
function checkNum(fm) {
	var startVal = fm.priceStart.value;
	var endVal = fm.priceEnd.value;
	if(!(parseInt(startVal) || parseInt(endVal))) {
		alert('请正确输入自定义的价格！');
		return false;
	}
	if(startVal.search(/\D/) >= 0 || parseInt(startVal) < 0) {//含有非数字格式
		alert('起始价格输入不正确！');
		fm.priceStart.value = '';
		return false;
	}
	if(endVal.search(/\D/) >= 0 || parseInt(endVal) < 0) {//含有非数字格式
		alert('结束价格输入不正确！');
		fm.priceEnd.value = '';
		return false;
	}
	if(parseInt(startVal) == 0 && parseInt(endVal) == 0) {
		alert('起始价格和结束价格不能都为0！');
		return false;
	}
	if(parseInt(startVal) >=  parseInt(endVal)) {
		alert('起始价格必须小于结束价格！');
		return false;
	}
	return true;
}
