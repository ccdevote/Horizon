		if (typeof($('#btnOpenCitiesLiebiao').val()) != "undefined") {
			var ipNewsDropLiebiao  = new DropCity('btnOpenCitiesLiebiao', 'btnCloseCitiesLiebiao', 'citiesListLiebiao', function(){				
