<script>
	var most = 18;
	/**
	* 显示当前城市
	*/		
	function showCurrent(info) {
		//curElem.innerHTML = info;
		$("#currentCity").html(info);
	}
	/**
	* 显示热门城市
	*/		
	function showHot(arr,areaId){
		var sites = IPInfo.groups, related = sites[IPInfo.site].related.toString();
		var len = most > arr.length ? arr.length : most;
		var paramStr = "&areaPinYin=";
		if(arr && arr.length > 0) {
			for(var i = 0; i < len; i++) {
				var key = arr[i], val = sites[key];
				if(related.indexOf(key) > -1 || key == IPInfo.site) {
					len ++;
					continue;				
				}
				//if(val) str += '<a href="http://product.pconline.com.cn/'+ val.alias+'/">' + key+ '</a>';
				paramStr += ""+val.alias+",";
			}
		}
		//alert("paramStr==="+paramStr);
		(function($){ 
			$.get('/_priceAreaNav.jsp?hotCity=true&smallTypeId=20937&groupType=2&url=/mobile/c11368/'+paramStr+"&areaId="+areaId,
			function(responseText){
				var str = '<i>热门城市：  </i> ';
				str  =str+ responseText.toString().replace('/(^\s*)|(\s*$)/g','');
				$("#hotCities").html(str);
				
		    } );
        })(jQuery);	
	}
	

	/**
	* 显示相关城市
	*/		
	function showRelated(arr,areaId) {
		var  sites = IPInfo.groups;
		var paramStr = "&areaPinYin=";
		if(arr && arr.length > 0) {
			for(var i = 0; i < arr.length; i++) {
				if(sites[arr[i]]) {
					//str += '<a href="http://product.pconline.com.cn/'+ sites[arr[i]].alias+'/">' + arr[i]+ '</a>';
					paramStr += ""+sites[arr[i]].alias+",";
					most--;
				}
			}
		}
		(function($){ 
			$.get('/_priceAreaNav.jsp?relatedCity=true&smallTypeId=20937&groupType=2&url=/mobile/c11368/'+paramStr+"&areaId="+areaId,
			function(responseText){
				var str = '<i>相关城市： </i>  ';
				str  = str+responseText.toString().replace('/(^\s*)|(\s*$)/g','');
				//relatedElem.innerHTML = str;
				$("#relatedCities").html(str);
		    } );
        })(jQuery);
        
         
		
	}
	
	//$(window).load(function(){

		if (typeof($('#btnOpenCities').val()) != "undefined") {
			var ipNewsDrop = new DropCity('btnOpenCities', 'btnCloseCities', 'citiesList', function(){
